{
  "name": "@treashunt/db",
  "version": "0.6.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist/**"],
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun --watch run build.ts",
    "db:migrate:deploy": "bun with-env prisma migrate deploy",
    "db:migrate:dev": "bun with-env prisma migrate dev",
    "db:push": "bun with-env prisma db push",
    "db:seed": "bun with-env prisma db seed",
    "db:migrate:force": "bun with-env prisma migrate reset --force",
    "format": "bun with-env prisma format",
    "generate": "bun with-env prisma generate",
    "prebuild": "bun run generate",
    "predev": "bun run generate",
    "studio": "bun with-env prisma studio",
    "with-env": "dotenv -e ../../.env --"
  },
  "prisma": {
    "seed": "bun src/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcryptjs": "^2.4.6",
    "dotenv-cli": "^7.4.1",
    "prisma": "^5.11.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  }
}
